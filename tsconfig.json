{
  "compilerOptions": {
                  
    "target": "es5",                         
    "module": "ES2020",                     
    "jsx": "react",                    
    "strict": true,                          
    "esModuleInterop": true,                 
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "outDir": "./dist",
    "moduleResolution": "node"
  }
}
